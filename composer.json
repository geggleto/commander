{
    "name": "commander/core",
    "description": "container-interop aware command-bus",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Glenn Eggeton",
            "email": "geggleto@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Commander\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Commander\\Test\\" : "tests"
        }
